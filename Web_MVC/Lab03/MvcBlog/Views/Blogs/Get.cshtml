@{
    Layout = null;
}
@model IEnumerable<EntityModel.Blog>

<h2>ViewBag.Title="Get"</h2>

<tr class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th></th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            </tr>
    }
</tr>
